{
  "name": "kungfu",
  "author": "taurus.ai",
  "version": "2.0.0",
  "description": "KungFu Trader App",
  "license": "Apache-2.0",
  "config": {
    "pyver": "three",
    "cmakejsopt": ""
  },
  "main": "./dist/app/main.js",
  "engines": {
    "node": ">=8.0.0 <11.0.0"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "clean": "rimraf ./dist/* ./build/*",
    "buildInstaller": "electron-builder",
    "build": "yarn run copy && node --max_old_space_size=8192 .electron-kungfu/build.js && electron-builder",
    "dev": "yarn run copy && node .electron-kungfu/dev-runner.js",
    "dist": "yarn run dist:render && yarn run dist:main",
    "dist:main": "cross-env NODE_ENV=production node --max_old_space_size=8192 ../node_modules/webpack/bin/webpack.js --progress --colors --config .electron-kungfu/webpack.main.config.js",
    "dist:render": "cross-env NODE_ENV=production node --max_old_space_size=8192 ../node_modules/webpack/bin/webpack.js --progress --colors --config .electron-kungfu/webpack.renderer.config.js",
    "copy": "copyfiles -f ../core/cpp/wingchun/resources/commission.db resources/default && copyfiles -f ../core/cpp/wingchun/resources/holidays.db resources/default"
  },
  "cmake-js": {
    "runtime": "electron",
    "runtimeVersion": "4.2.0",
    "arch": "x64"
  },
  "build": {
    "productName": "Kungfu.Trader",
    "artifactName": "${productName}-${version}.${ext}",
    "appId": "taurus.ai.KungFu.Trader",
    "directories": {
      "output": "build/app"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://taurusai.com/kungfu"
      }
    ],
    "files": [
      "dist/app/**/*",
      {
        "from": ".",
        "to": "dist/app",
        "filter": [
          "resources"
        ]
      },
      {
        "from": "../core/cpp/wingchun/resources",
        "to": "dist/app/resources/default",
        "filter": [
          "commission.db",
          "holidays.db"
        ]
      },
      "!**/kungfu-core/*",
      "!**/kungfu-core/**/*",
      "!**/kungfu-core/**/**/*",
      "!**/kungfu-core/**/**/*",
      "**/kungfu-core/package.json",
      "**/kungfu-core/build/Release/node_nanomsg.node",
      "**/kungfu-core/build/Debug/node_nanomsg.node",
      "**/kungfu-core/node/*"
    ],
    "extraResources": [
      {
        "from": "../core/build/kfc",
        "to": "kfc"
      },
      {
        "from": "../node_modules/kungfu-core",
        "to": "app.asar.unpacked/node_modules/kungfu-core",
        "filter": [
          "node/*",
          "build/**/node_nanomsg.node",
          "package.json"
        ]
      }
    ],
    "asar": true,
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "./resources/logo/icon.icns",
      "type": "distribution",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "icon": "./resources/logo/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "icon": "./resources/logo"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./resources/logo/icon.ico",
      "uninstallerIcon": "./resources/logo/icon.ico",
      "installerHeaderIcon": "./resources/logo/icon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "bindings": "^1.5.0",
    "electron-updater": "^4.0.6",
    "fast-csv": "^2.5.0",
    "fkill": "^6.1.0",
    "fs-extra": "^7.0.1",
    "getprocesses": "^1.1.1",
    "log4js": "^4.0.2",
    "mime": "^2.4.0",
    "moment": "^2.24.0",
    "pm2": "^3.5.0",
    "readline": "^1.3.0",
    "rxjs": "^6.5.2",
    "sqlite3": "^4.0.6",
    "tail": "^2.0.2",
    "taskkill": "^2.0.0",
    "kungfu-core": ""
  },
  "devDependencies": {
    "codemirror": "^5.44.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.1",
    "vue-virtual-scroller": "^1.0.0-rc.2",
    "vuex": "^3.0.1",
    "element-ui": "^2.8.2",
    "colors": "^1.3.3",
    "aws-sdk": "^2.395.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chai": "^4.0.0",
    "chalk": "^2.1.0",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.0.5",
    "cross-var": "^1.1.0",
    "css-loader": "^2.1.1",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "4.2.0",
    "electron-builder": "^20.38.5",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.0",
    "mkdirp": "^0.5.1",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.12.0",
    "optimize-js-plugin": "^0.0.4",
    "preload-webpack-plugin": "^2.3.0",
    "require-dir": "^0.3.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.0.3",
    "spectron": "^3.7.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.1.2",
    "vue-electron": "^1.0.6",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.2",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-dev-server": "3.0.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0",
    "webpack-permissions-plugin": "^1.0.0"
  }
}
